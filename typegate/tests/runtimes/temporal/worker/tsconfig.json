{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext",
      "ESNext.Array",
      "ESNext.Promise",
      "ESNext.AsyncIterable"
    ],
    "module": "Node16",
    "rootDir": "./",
    "moduleResolution": "Node16",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
