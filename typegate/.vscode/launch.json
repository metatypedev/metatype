{
  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Test prisma",
      "type": "pwa-node",
      "program": "${workspaceFolder}/tests/prisma_test.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/home/remote/.cargo/bin/deno",
      "runtimeArgs": [
        "test",
        "--unstable",
        "--import-map=import_map.json",
        "--inspect",
        "--allow-all",
        "--filter=relationships"
      ],
      "attachSimplePort": 9229
    }
  ]
}
